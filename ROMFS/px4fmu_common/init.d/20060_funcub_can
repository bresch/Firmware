#!nsh
#
# @name Fun Cub Quad VTOL CAN Example
# @type Standard VTOL
# @class VTOL
# @maintainer Andreas Antener <andreas@uaventure.com>
# @output CAN Quad X
# @output MAIN1 Aileron 1
# @output MAIN2 Aileron 2
# @output MAIN3 Elevator
# @output MAIN4 Rudder
# @output MAIN5 Throttle
# @output AUX1 Aileron 1
# @output AUX2 Aileron 2
# @output AUX3 Elevator
# @output AUX4 Rudder
# @output AUX5 Throttle
#

sh /etc/init.d/rc.vtol_defaults

if [ $AUTOCNF == yes ]
then
    param set VT_TYPE 2
    # 0 motor count (PWM) as they are on CAN
    param set VT_MOT_COUNT 0
    param set VT_TRANS_THR 0.75
    param set VT_ARSP_TRANS 12
    param set VT_ARSP_BLEND 6

    param set PWM_DISARMED 950
    param set PWM_AUX_DISARMED 950
    param set PWM_RATE 50

    param set MC_ROLL_P 6.0
    param set MC_ROLLRATE_P 0.17
    param set MC_ROLLRATE_I 0.002
    param set MC_ROLLRATE_D 0.004
    param set MC_ROLLRATE_FF 0.0
    param set MC_PITCH_P 6.0
    param set MC_PITCHRATE_P 0.19
    param set MC_PITCHRATE_I 0.002
    param set MC_PITCHRATE_D 0.004
    param set MC_PITCHRATE_FF 0.0
    param set MC_YAW_P 2.8
    param set MC_YAW_FF 0.5
    param set MC_YAWRATE_P 0.22
    param set MC_YAWRATE_I 0.02
    param set MC_YAWRATE_D 0.0
    param set MC_YAWRATE_FF 0.0
    param set MC_YAWRATE_MAX 40
    param set MC_YAWRAUTO_MAX 40

    param set FW_PR_FF 0.5
    param set FW_PR_I 0.02
    param set FW_PR_IMAX 0.4
    param set FW_PR_P 0.08
    param set FW_RR_FF 0.6
    param set FW_RR_I 0.01
    param set FW_RR_IMAX 0.2
    param set FW_RR_P 0.05
    param set FW_THR_CRUISE 0.75
fi

set MIXER_CAN vtol_quad_x

# Using the same mixer on IO and FMU (as an example)
set MIXER vtol_AAERT
set PWM_OUT 12345678
set PWM_MCHDIS 5
set PWM_RATE p:PWM_RATE

set MIXER_AUX vtol_AAERT
set PWM_AUX_OUT 123456
set PWM_ACHDIS 5

set MAV_TYPE 22
